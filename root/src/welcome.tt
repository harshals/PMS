            $(document).ready(function() {
                init_sidebar_forms();
            });
		 </script>

	</head>
	<body>
		<div class="container_16">
			
			<div class="clear"></div>
			<!-- sidebar form container -->
			<div class="grid_4 sidebar">

				[% INCLUDE logo %]	
				
				[% IF c.user_exists%]
				[% INCLUDE help/section_menu %]
				[% END %]
				
                [% INCLUDE profile_image %]
			<div class="block"> 
			</div>	

           	<!-- sidebar form container-->


			</div><!-- grid_4 -->
