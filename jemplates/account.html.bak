<div id="sidebar-content" class="">
    <h2>Add Account No.</h2>
    <form class="yellow box">
     <fieldset>
			 <ul class="form vvv thin">
				   <li>
					   <select name="add_account" style="width: 167px;">
						   [% FOREACH acc IN  data %] 
							   <option value="[% acc.account_no %]" >
												[% acc.account_no %]
							   </option>
						   [% END %]
						 </select>
				   </li>
				   
		      </ul>
       </fieldset> 
            <li class="form hii">
		      <input type="button" id="add" value="ADD" class="button" />
			<input type="reset" name="reset" value="Reset" class="button" />
		</li>
   </form>

</div>

